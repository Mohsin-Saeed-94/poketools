{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://poketools.gamestuff.info/data/schema/range.json",
  "oneOf": [
    {
      "type": "integer",
      "minimum": 1,
      "maximum": 100
    },
    {
      "type": "string",
      "minLength": 3,
      "pattern": "^([0-9]+)-([0-9]+)$"
    }
  ]
}
